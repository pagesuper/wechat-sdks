{
  "name": "wechat-sdks",
  "version": "1.0.1",
  "description": "WeChat SDKs",
  "author": "happy",
  "private": false,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/pagesuper/wechat-sdks"
  },
  "_moduleAliases": {
    "@root": "."
  },
  "scripts": {
    "eslint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore .",
    "eslint:fix": "yarn eslint --fix",
    "stylelint": "stylelint \"{src,source}/**/*.{css,scss,sass}\" --allow-empty-input",
    "stylelint:fix": "yarn stylelint --fix",
    "lint": "yarn run eslint && yarn run stylelint",
    "lint:fix": "yarn run eslint:fix && yarn run stylelint:fix",
    "tsc": "./node_modules/.bin/tsc -p tsconfig.json",
    "check": "npm run test && npm run eslint",
    "test": "jest --clearCache && jest --runInBand --detectOpenHandles"
  },
  "typings": "types/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --ext .js,.ts,.vue --ignore-path .eslintignore . --fix"
    ],
    "*.{css,scss}": "stylelint --fix"
  },
  "dependencies": {
    "almighty-tool": ">=0.0.13"
  },
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "7.20.0",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-typescript": "7.18.6",
    "@types/cache-manager": "4.0.2",
    "@types/cookie": "0.6.0",
    "@types/html5plus": "1.0.5",
    "@types/jest": "29.5.12",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.16",
    "@types/power-assert": "1.5.12",
    "@types/qs": "6.9.11",
    "@types/request-ip": "0.0.41",
    "@types/uuid": "9.0.8",
    "@types/webpack-env": "1.18.4",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-nuxt": ">=3.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "9.21.1",
    "eslint-plugin-vue-scoped-css": "2.7.2",
    "jest": "29.4.3",
    "lint-staged": "15.2.1",
    "power-assert": "^1.6.1",
    "prettier": "3.2.4",
    "stylelint": "16.2.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "36.0.0",
    "stylelint-config-standard-scss": "13.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.1.0",
    "ts-jest": "29.1.2",
    "webpack-env": "0.8.0",
    "yorkie": "2.0.0"
  }
}
